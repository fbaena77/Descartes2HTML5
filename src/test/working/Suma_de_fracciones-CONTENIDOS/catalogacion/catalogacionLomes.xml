                    	<lomes:string  language="es">fracciones</lomes:string>

                    	<lomes:string  language="es">numerador</lomes:string>

                    	<lomes:string  language="es">denominador</lomes:string>

                    	<lomes:string  language="es">suma</lomes:string>

                    	<lomes:string  language="es">producto cruzado</lomes:string>

                    	<lomes:string  language="es">parte</lomes:string>

